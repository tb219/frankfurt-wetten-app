{
  "version": 2,
  "builds": [
    {
      "src": "api/app.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/betting-recommendation",
      "dest": "api/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
